{
	"name": "web-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"fdc:download": "node scripts/fdc-etl/download-fdc-data.js",
		"fdc:translate": "node scripts/fdc-etl/add-translations.js",
		"fdc:migrate": "node scripts/fdc-etl/migrate-to-food.js",
		"fdc:migrate:dry": "node scripts/fdc-etl/migrate-to-food.js --dry-run",
		"fdc:all": "npm run fdc:download && npm run fdc:translate && npm run fdc:migrate"
	},
	"dependencies": {
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.50.5",
		"dotenv": "^16.4.5",
		"mercadopago": "^2.0.15",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.4.31",
		"sharp": "^0.34.4",
		"svelte": "^5.0.0",
		"tailwindcss": "^3.4.4",
		"vite": "^7.0.4"
	},
	"devDependencies": {
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0"
	}
}
